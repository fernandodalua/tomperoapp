
<!doctype html>
<html lang="pt-br">
  <head>
  <% include partials/head %>
  </head>

  <body>
	<% include partials/menu %>
	<!-- Begin page content -->
  <main role="main" class="container">
    <div class="container mt-5 mb-5">
      <div class="row mb-3">
        <div class="col-md-3">
          <div class="card flex-md-row mb-4 box-shadow h-md-250">
            <div class="card-body d-flex flex-column align-items-start">
              
            </div>
          </div>
        </div>
        <div class="col-md-6">
          <br>
            <h4>Meu cardápio</h4>
            <ul class="timeline">
              <% for(var i = 0; i < feedUser.length; i++){ %>
              <li>
                <a href="/profile/"<%= feedUser[i].id_account %>"><%= feedUser[i].fullname %>
                  </a>
                <a href="#" class="float-right">
                  <%= feedUser[i].date_post %>
                  </a>
                <div class="card sticky-top" style="margin:0; border: none !important;">
                  <p>
                    <%= feedUser[i].post %>
                    </p>
                  <%if (feedUser=""[i=""].photo="") { %>
                    <img src="img/"<%=feedUser[i].photo%>" class="img-thumbnail">
                      <%}%>
                  </div>
              </li>
              <% } %>
            </ul>
          </div>
        <div class="col-md-3">
          <div class="card flex-md-row mb-4 box-shadow h-md-250">
            <div class="card-body d-flex flex-column align-items-start">
              <strong class="d-inline-block mb-2 text-primary">Prato do dia</strong>
              <h3 class="mb-0">
                <a class="text-dark" href="#">Érick Jacquin</a>
              </h3>
              <div class="mb-1 text-muted">TARTAR DE SALMÃO</div>
              <p class="card-text mb-auto">Com manga, ervas frescas e limão siciliano</p>
              <a href="#">Continue lendo</a>
            </div>
          </div>
        </div>
      </div>
    </div>
	</main>
  <footer class="footer">
    <div class="container">
      <!-- FOOTER -->
    </div>
  </footer>
